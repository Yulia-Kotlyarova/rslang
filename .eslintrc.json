{
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "ignorePatterns": ["dist/", "node_modules/"],
    "rules": {
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    },
    "overrides": [
        {
            "files": [
                "*.js"
            ],
            "rules": {
                "linebreak-style": ["error", "windows"]
            }
        }
    ]
}
